SAFT=../saft/saft
function echo_saft () { echo ""; echo saft $1; $SAFT $1; }
echo_saft ""
echo_saft "--help"
echo_saft "-h"
echo_saft "--version"
echo_saft "-V"
echo_saft "--verbose"
echo_saft "-v"
echo_saft "--help --version"
echo_saft "--version --verbose"
echo_saft "--verbose --help"
echo_saft "--help -V"
echo_saft "--version -v"
echo_saft "--verbose -h"
echo_saft "--program saftn --input test_input.fna"
echo_saft "--program saftn --database test_database.fna"
echo_saft "--program saftn --input --database test_database.fna"
echo_saft "--program saftn --input test_input.fna --database"
echo_saft "--program saftn --input missing_file --database test_database.fna"
echo_saft "--program saftn --input test_input.fna --database missing_file"
echo_saft "--program saftn --input test_input.fna --database test_database.fna"
echo_saft "--program saftp --input test_input.faa --database test_database.faa"
echo_saft "--program wrong --input test_input.fna --database test_database.fna"
echo_saft "-p saftn -i test_input.fna -d test_database.fna"
echo_saft "-p saftn -i test_input.fna -d test_database.faa"
echo_saft "-p saftn -i test_input.faa -d test_database.fna"
echo_saft "-p saftn -i test_input.faa -d test_database.faa"
echo_saft "-p saftp -i test_input.fna -d test_database.fna"
echo_saft "-p saftp -i test_input.fna -d test_database.faa"
echo_saft "-p saftp -i test_input.faa -d test_database.fna"
echo_saft "-p saftp -i test_input.faa -d test_database.faa"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --help"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --version"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --verbose"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --statistic"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -s"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --statistic wrong"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --statistic d2"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --statistic d2ast"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --statistic d2c"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --statistic d2dag"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --statistic d2w"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --statistic d2wc"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize 2"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize 2 --statistic wrong"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize 2 --statistic d2"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize 2 --statistic d2ast"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize 2 --statistic d2c"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize 2 --statistic d2dag"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize 2 --statistic d2w"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --wordsize 2 --statistic d2wc"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -w"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -w 2"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --showmax"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --showmax 2"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -b"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -b 2"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --pmax"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --pmax 1"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -e"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -e 1"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --lockstep"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -l"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --raw"
echo_saft "--program saftn --input test_input.fna --database test_database.fna -r"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --pmax 1 --raw"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --pmax 1 --lockstep"
echo_saft "--program saftn --input test_input.fna --database test_database.fna --pmax 1 --wordsize 2"
